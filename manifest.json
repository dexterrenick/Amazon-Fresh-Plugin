{
    "name": "Amazon Fresh Carbon Cost",
    "description": "Estimate C02 emissions of items on the Amazon Fresh website.",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "author": "Dexter Renick",
    "icons": { "16": "logo.png",
           "48": "logo.png",
          "128": "logo.png" },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "all_frames": true,
        "js": ["content_script.js"],
        "run_at": "document_end"
      }
    ]
}
